<%- include("./partials/header"); -%>
<title>add a new movie</title>
<div class="content">
  <h1 class="heading">Add a Movie</h1>
  <form method="post" action="/add">
    <input
      type="text"
      id="movie"
      placeholder="enter movie name"
      name="movie"
      autofocus
    />
    <br /><br />
    <input class="btn btn-primary btn-md" value="Add Movie" type="submit" />
  </form>
</div>

